<!DOCTYPE html>

<html lang="es-mx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="Panel Administrador - PaperMID">
    <meta name="author" content="Jesús Huchim - UTM">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/Images/favicon.png">

    <title>@ViewBag.Title</title>
    <script src="~/Scripts/jquery-3.2.1.js" type="text/javascript"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/tether.min.js" type="text/javascript"></script>
    <link href="~/Content/Css/bootstrap.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/bootstrap.js" type="text/javascript"></script>

    <!-- Style Template -->
    <link href="~/Content/Css/Admin-style.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Css/blue.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Css/chartist.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Css/chartist-plugin-tooltip.css" rel="stylesheet" type="text/css" />

    <!-- Javascript -->

    <script src="~/Scripts/jquery.slimscroll.js" type="text/javascript"></script>
    <script src="~/Scripts/sidebarmenu.js" type="text/javascript"></script>
    <script src="~/Scripts/dashboard1.js" type="text/javascript"></script>
    <script src="~/Scripts/waves.js" type="text/javascript"></script>
    <script src="~/Scripts/sticky-kit.js" type="text/javascript"></script>
    <script src="~/Scripts/custom.js" type="text/javascript"></script>
    <script src="~/Scripts/c3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/d3.js" type="text/javascript"></script>
    <link href="~/Content/Css/animate.css" rel="stylesheet" />
    <!-- Fontawesome Icons -->
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet">
    <!-- Alertify -->
    <link href="~/Content/Css/alertify.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/alertify.js" type="text/javascript"></script>
    <!-- Jquery UI -->
    <link href="~/Content/Css/jquery-ui.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui.js" type="text/javascript"></script>

    <!-- Sweetalert -->
    <script src="~/Scripts/sweetalert.js" type="text/javascript"></script>
    <!-- Modificaciones css y scripts -->
    <link href="~/Content/Css/Administrador.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/Administrador.js" type="text/javascript"></script>

    <link href="~/Content/Css/jquery.dataTables.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery.dataTables.js" type="text/javascript"></script>

    <!--Jquey Validate-->
    <script src="~/Scripts/jquery.validate.js" type="text/javascript"></script>
    @{
        if (Session["IdUsuario"] != null)
        {
            string User = Session["IdUsuario"].ToString();
        }
        else
        {
            Response.Redirect("~/Publico/Inicio");
        }
    }

</head>
<body class="fix-header fix-sidebar card-no-border">
    <!-- Preloader -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- ./ Preloader -->
    <!-- Main-wrapper -->
    <div id="main-wrapper">
        <!-- Header -->
        <header class="topbar">
            <nav class="navbar top-navbar navbar-toggleable-sm navbar-light">

                <div class="navbar-header">
                    <a class="navbar-brand" href="index.html">
                        <!-- Logo icon --><b>
                            <!-- Light Logo icon -->
                            <img src="~/Content/Images/logo-light-icon.png" alt="homepage" class="light-logo" />
                        </b>
                        <!--End Logo icon -->
                        <!-- Logo text --><span>

                            <!-- Light Logo text -->
                            <img src="~/Content/Images/logo-light-text.png" class="light-logo" alt="homepage" />
                        </span>
                    </a>
                </div>
                <div class="navbar-collapse">
                    <!-- ============================================================== -->
                    <!-- toggle and nav items -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav mr-auto mt-md-0">
                        <!-- This is  -->
                        <li class="nav-item"> <a class="nav-link nav-toggler hidden-md-up text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="mdi mdi-menu"></i></a> </li>
                        <!-- ============================================================== -->
                        <!-- Search -->
                        <!-- ============================================================== -->
                        <li class="nav-item hidden-sm-down search-box">
                            <a class="nav-link hidden-sm-down text-muted waves-effect waves-dark" href="javascript:void(0)"><i class="ti-search"></i></a>
                            <form class="app-search">
                                <input type="text" class="form-control" placeholder="Search & enter"> <a class="srh-btn"><i class="ti-close"></i></a>
                            </form>
                        </li>
                    </ul>
                    <!-- ============================================================== -->
                    <!-- User profile and search -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav my-lg-0">
                        <!-- ============================================================== -->
                        <!-- Profile -->
                        <!-- ============================================================== -->
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-muted waves-effect waves-dark" href="" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Administrador</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- ./ Header -->
        <aside class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Inicio" aria-expanded="false"><i class="fa fa-home"></i><span class="hide-menu">&nbsp;&nbsp;Inicio</span></a>
                        </li>
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Venta" aria-expanded="false"><i class="fa fa-shopping-cart"></i><span class="hide-menu">&nbsp;Punto de Venta</span></a>
                        </li>
                        <!---->
                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="#" aria-expanded="false"><i class="fa fa-clipboard"></i><span class="hide-menu">Inventario</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Inventario/Productos">Productos</a></li>
                                <li><a href="/Inventario/Promociones">Promociones</a></li>
                                <li><a href="/Inventario/Ingresos">Ingresos</a></li>
                                <li><a href="/Inventario/Egresos">Egresos</a></li>
                            </ul>
                        </li>
                        <!---->
                        <li>
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Departamentos" aria-expanded="false"><i class="fa fa-clone"></i><span class="hide-menu">&nbsp;Departamentos</span></a>
                        </li>
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Proveedores" aria-expanded="false"><i class="fa fa-truck"></i><span class="hide-menu">&nbsp;Proveedores</span></a>
                        </li>
                        <li>
                            <a class="waves-effect waves-dark" href="pages-blank.html" aria-expanded="false"><i class="fa fa-file-alt"></i><span class="hide-menu">&nbsp;Reportes</span></a>
                        </li>
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Empleados" aria-expanded="false"><i class="fa fa-user"></i><span class="hide-menu">&nbsp;Empleados</span></a>
                        </li>
                        <li>
                            <a class="waves-effect waves-dark" href="/Administrador/Papeleria" aria-expanded="false"><i class="fa fa-info-circle"></i><span class="hide-menu">&nbsp;Datos Papelería</span></a>
                        </li>
                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
            <!-- Bottom points-->
            <div class="sidebar-footer">
                <!-- item--><a href="" class="link" data-toggle="tooltip" title="Settings"><i class="fa fa-cogs"></i></a>
                <!-- item--><a href="" class="link" data-toggle="tooltip" title="Email"><i class="fa fa-envelope"></i></a>
                <!-- item-->@*<a href="" class="link" data-toggle="modal" title="Cerrar sesión" data-target="#modalLogoutForm"><i class="mdi mdi-power"></i></a>*@
                <a href="@Url.Action("Logout","Publico")" class="link" data-toggle="tooltip" title="Cerrar sesión"><i class="fa fa-sign-out-alt"></i></a>
            </div>
            <!-- End Bottom points-->
        </aside>
        <div class="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
            <footer class="footer">
                © 2018 Papelería Mérida
            </footer>
        </div>

    </div>
    <!-- ./ Main-wrapper -->
</body>
</html>
